<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gallery.studios.endive.com.gallerynew">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FirstActivity"
            android:label="Gallery" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:theme="@style/AppTheme"></activity>
        <activity android:name=".GalleryActivity" android:theme="@style/AppTheme"></activity>
        <activity android:name=".AboutActivity" android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
